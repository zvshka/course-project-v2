{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@hooks": [
        "hooks",
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@lib": [
        "lib"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@services": [
        "services"
      ],
      "@services/*": [
        "services/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
